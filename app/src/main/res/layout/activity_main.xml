<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/paramInput"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:hint="初次加载RN传递的数据" />


    <Button
        android:id="@+id/bt_jump1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/paramInput"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:text="启动一个FlutterActivity直接跳转" />

    <Button
        android:id="@+id/bt_jump"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bt_jump1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:text="复写FlutterActivity方式（自定义插件）" />


</RelativeLayout>